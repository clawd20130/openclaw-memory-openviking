{
  "name": "@kevinzhow/openclaw-memory-openviking",
  "version": "0.1.1",
  "description": "OpenViking context database as memory backend for OpenClaw",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md",
    "LICENSE"
  ],
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "test": "npm run build && node --import tsx --test tests/*.test.ts",
    "lint": "npm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "memory",
    "openviking",
    "context-database",
    "agent-memory"
  ],
  "author": "Kevin Zhow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinzhow/openclaw-memory-openviking.git"
  },
  "bugs": {
    "url": "https://github.com/kevinzhow/openclaw-memory-openviking/issues"
  },
  "homepage": "https://github.com/kevinzhow/openclaw-memory-openviking#readme",
  "dependencies": {
    "@sinclair/typebox": "0.34.48"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.15"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "openclaw": "^2026.2.15",
    "tsx": "^4.20.6",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
